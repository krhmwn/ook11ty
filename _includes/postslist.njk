<ol class="postlist">{% for post in postslist | reverse %}<li class="postlist-item">
<h2><a href="{{ post.url | url }}" title="{{ post.data.title }}">{% if post.data.title %}{{ post.data.title }}{% else %}<code>{{ post.url }}</code>{% endif %}</a></h2>
<time datetime="{{ post.date | htmlDateString }}">{{ post.date | readableDate }}</time>
{% for tag in post.data.tags | filterTagList %}{% set tagUrl %}/tags/{{ tag | slug }}/{% endset %}<span class="post-tag" title="{{ tag }}">{{ tag }}</span>{% endfor %}
</li>{% endfor %}
</ol>
